{
  "name": "asl-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "start-dev": "nodemon dist/index.js",
    "test": "rm -f asl-unittest.sqlite && NODE_ENV=unittest knex migrate:latest && NODE_ENV=unittest knex seed:run && NODE_ENV=unittest mocha --reporter spec --compilers ts:ts-node/register 'test/**/*.test.ts'",
    "migrate": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "testmigrate": "NODE_ENV=unittest knex migrate:latest",
    "testmigrate-rollback": "NODE_ENV=unittest knex migrate:rollback"
  },
  "dependencies": {
    "@types/knex": "0.0.53",
    "body-parser": "1.15.2",
    "bookshelf": "0.10.3",
    "bookshelf-eloquent": "0.1.4",
    "cookie-parser": "1.4.3",
    "debug": "2.6.3",
    "express": "4.14.0",
    "jade": "1.11.0",
    "joi": "10.6.0",
    "knex": "0.12.0",
    "knex-filter-loopback": "0.1.0",
    "morgan": "1.7.0",
    "mysql": "2.13.0",
    "serve-favicon": "2.4.2",
    "typescript": "2.3.4"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/debug": "0.0.29",
    "@types/express": "4.0.33",
    "@types/mocha": "2.2.32",
    "@types/morgan": "1.7.32",
    "@types/node": "6.0.46",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "gulp": "3.9.1",
    "gulp-typescript": "3.1.1",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "sqlite3": "3.1.8",
    "ts-node": "1.6.1"
  }
}
